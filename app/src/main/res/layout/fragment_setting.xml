<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/grey"
    tools:context="com.example.imtisal.finalprojectv5.SettingFragment">

    <!-- TODO: Update blank fragment layout -->


    <Button
        android:text="Volvo Office"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:alpha="0.33"
        android:textSize="20dp"
        android:id="@+id/volvoDir"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Daewoo Office"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:alpha="0.33"
        android:textSize="20dp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:id="@+id/daewooDir"
        android:layout_above="@+id/volvoDir"
        android:layout_alignParentStart="true" />

    <Button
        android:text="Niazi Office"
        android:layout_width="match_parent"
        android:layout_height="150dp"
        android:textSize="20dp"
        android:alpha="0.33"
        android:id="@+id/niaziDir"
        android:layout_below="@+id/volvoDir"
        android:layout_alignParentStart="true"
        android:layout_marginTop="11dp"
        android:textStyle="bold"/>
</RelativeLayout>
